<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>Question</from-outcome>
			<to-view-id>/admin/Question.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>ListQuestion</from-outcome>
			<to-view-id>/admin/ListQuestion.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>Theme</from-outcome>
			<to-view-id>/admin/Theme.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>Login</from-outcome>
			<to-view-id>/auth/Login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>RegisterCandidat</from-outcome>
			<to-view-id>/auth/RegisterCandidat.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>Candidat</from-outcome>
			<to-view-id>/admin/Candidat.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>RegisterEntreprise</from-outcome>
			<to-view-id>/auth/RegisterEntreprise.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>Entreprise</from-outcome>
			<to-view-id>/admin/Entreprise.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>Password</from-outcome>
			<to-view-id>/shared/Password.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>EntrepriseProfile</from-outcome>
			<to-view-id>/entreprise/Profile.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>CandidatProfile</from-outcome>
			<to-view-id>/candidat/Profile.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>AdminProfile</from-outcome>
			<to-view-id>/admin/Profile.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<application />
</faces-config>
